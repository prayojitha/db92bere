
extends layout

block content
  if(!user)
    a(href="/login") login
    br
    a(href="/register") register
  if(user)
    p You are currently logged in as #{user.username} 
    a(href="/electronic") Take me to electronic 
    br
    a(href="/logout") Logout 


  h1= title

  p Welcome to #{title}
  p buttons.